
num1 = float(input("Enter first number: "))
num2 = float(input("Enter second number: "))

print("\nBefore swapping:")
print("First number:", num1)
print("Second number:", num2)

temp = num1
num1 = num2
num2 = temp

print("\nAfter swapping:")
print("First number:", num1)
print("Second number:", num2)
